{
  "layout": {
    "header": {
      "type": "Header",
      "properties": {
        "title": "Project Dashboard",
        "menu": [
          { "name": "Home", "link": "/" },
          { "name": "Projects", "link": "/projects" },
          { "name": "Teams", "link": "/teams" },
          { "name": "Reports", "link": "/reports" }
        ]
      }
    },
    "footer": {
      "type": "Footer",
      "properties": {
        "text": "Â© 2023 Project Management Dashboard"
      }
    },
    "content": {
      "type": "Layout",
      "properties": {
        "style": { "padding": "24px" },
        "content": [
          {
            "type": "Row",
            "properties": {
              "gutter": 16,
              "children": [
                {
                  "type": "Col",
                  "span": 8,
                  "content": { "type": "Card", "title": "Ongoing Projects" }
                },
                {
                  "type": "Col",
                  "span": 8,
                  "content": { "type": "Card", "title": "Upcoming Meetings" }
                },
                {
                  "type": "Col",
                  "span": 8,
                  "content": { "type": "Card", "title": "Recent Activities" }
                }
              ]
            }
          }
        ]
      }
    }
  },
  "navigation": {
    "type": "Breadcrumb",
    "properties": {
      "items": [
        { "name": "Home", "link": "/" },
        { "name": "Projects", "link": "/projects" }
      ]
    }
  },
  "dataEntry": {
    "type": "Form",
    "properties": {
      "layout": "vertical",
      "items": [
        { "label": "Project Name", "type": "Input" },
        { "label": "Start Date", "type": "DatePicker" },
        { "label": "End Date", "type": "DatePicker" },
        {
          "label": "Status",
          "type": "Select",
          "options": ["Planning", "Active", "Completed"]
        },
        { "label": "Description", "type": "TextArea" }
      ],
      "submitButton": "Submit"
    }
  },
  "dataDisplay": {
    "type": "Table",
    "properties": {
      "columns": [
        { "title": "Name", "dataIndex": "name", "key": "name" },
        { "title": "Status", "dataIndex": "status", "key": "status" },
        { "title": "Start Date", "dataIndex": "startDate", "key": "startDate" },
        { "title": "End Date", "dataIndex": "endDate", "key": "endDate" }
      ],
      "dataSource": []
    }
  },
  "feedback": {
    "type": "Modal",
    "properties": {
      "title": "Notification",
      "content": "Your changes have been saved."
    }
  },
  "general": {
    "type": "Avatar",
    "properties": {
      "icon": "user",
      "size": "large"
    }
  }
}
